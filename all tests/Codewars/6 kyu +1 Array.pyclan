массив === (ар) {
	если не ар:
		воздать "нул";
	для с в ар:
		если с меньше 0 или с паче 9:
			воздать "нул";
	
	деяти ар.обратно();
	где ар[0] = ар[0] + 1;
	
 	воздать ар.перебор(=== эл, ин: {
		если ар[ин] паче 9 {
			где ар[ин] = 0;
			если !(ин + 1 меньше ар.длина()):
				ар += 1;
			иначе:
				где ар[ин + 1] = ар[ин + 1] + 1;
		}
		воздать ар[ин];
	}).обратно();
}

массив([2,3,9]);
массив([9,9]);
массив([0,4,2]);
массив([4,3,2,5]);
массив([1,2,3,9]);
массив([9,9,9,9]);
массив([0,1,3,7]);
массив([1,-9]);
массив([1,33]);
массив([]);
