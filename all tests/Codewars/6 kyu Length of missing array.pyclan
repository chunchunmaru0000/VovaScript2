длина_что_нету === (массивы) {
	если не массивы или массивы.имеет([]):
		воздать 0;
	м = массивы.порядком(=== эл: эл.длина());
	для ин в от 0 до м.длина() - 1:
		если м[ин + 1].длина() - м[ин].длина() паче 1:
			воздать м[ин].длина() + 1;
}

длина_что_нету([[1, 2], [4, 5, 1, 1], [1], [5, 6, 7, 8, 9]]);
длина_что_нету([['a', 'a', 'a'], ['a', 'a'], ['a', 'a', 'a', 'a'], ['a'], ['a', 'a', 'a', 'a','a', 'a']]);
