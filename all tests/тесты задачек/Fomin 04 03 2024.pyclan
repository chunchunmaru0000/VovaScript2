поиск_перебор === (начало, конец, найти){
	реквием {
		если начало == конец:
			воздать "нашел";
		++начало;
		если начало паче конец:
			воздать "не нашел";
	}
}

поиск_рандомом === (начало, конец, найти){
	циклов = 0;
	реквием {
		++циклов;
		если случайный(начало, конец) == найти {
			воздать "за " + циклов + " нашел"; 
		}
	}
}
	
реквием {
	начало = хартия("начало: ").числом();
	конец = хартия("конец: ").числом();
	найти = хартия("найти: ").числом();
	
	когдато = сейчас;
	"идет поиск перебором...";
	поиск_перебор(начало, конец, найти);
	"поиск перебором занял: " + датой(сейчас - когдато);
	
	когдато = сейчас;
	"идет поиск рандомом...";
	поиск_рандомом(начало, конец, найти);
	"поиск рандомом занял: " + датой(сейчас - когдато);
}

