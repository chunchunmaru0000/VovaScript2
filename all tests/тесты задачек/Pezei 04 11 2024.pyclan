реквием {
	массив = [];
	числа = [];
	н = хартия("чмсло н: ").числом();
	для сч в от 1 до макс(5, н):
		массив += случайный(1, н + 1);

	"массив: " + массив.строкой();
	
	найдёт = истина;
	если массив.суммой() меньше н:
		найдёт = ложь;
	если массив.фильтр(=== эл: эл меньше н).длина() == 0:
		найдёт = ложь;

	если найдёт {
		дв_число = двоичным(1, массив.длина());
		
		пока не (дв_число.фильтр(=== эл: эл != "1").длина() == 0) {
			чсла = массив.фильтр(=== эл, ин: дв_число[ин] == 1);

			если чсла.суммой().числом() == н:
				числа += [чсла];
			дв_число = двоич_плюс(дв_число, 1);
		}
	}
	
	чмсла = [];
	для ч в числа:
		если не чмсла.имеет(ч):
			чмсла += [ч];
			
	"числа: " + чмсла.строкой();	
}